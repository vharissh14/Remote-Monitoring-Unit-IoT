<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script type="text/javascript">
google.charts.load('current', {'packages':['gauge']});
google.charts.setOnLoadCallback(drawChart);

function drawChart() {
  var socket = io.connect('http://localhost:5000/');
  var motor_chart = {
    volt: google.visualization.arrayToDataTable([['Label', 'Value'],['Volt (V)', 0]]),
    current: google.visualization.arrayToDataTable([['Label', 'Value'],['Current (A)', 0]]),
    power: google.visualization.arrayToDataTable([['Label', 'Value'],['Power (KW)', 0]]),
    freq: google.visualization.arrayToDataTable([['Label', 'Value'],['Freq (Hz)', 0]]),
    rpm: google.visualization.arrayToDataTable([['Label', 'Value'],['RPM x 1000', 0]])
  }

  var motor_options = {
    volt: {width: 200, height: 200, min:0, max: 500, majorTicks: ['0', '100', '200', '300', '400', '500']},
    current: {width: 200, height: 200, min:0, max: 20, majorTicks: ['0', '5', '10', '15', '20']},
    power: {width: 200, height: 200, min:0, max: 20, majorTicks: ['0', '5', '10', '15', '20']},
    freq: {width: 200, height: 200, min:0, max: 300, majorTicks: ['0', '100', '200', '300']},
    rpm: {width: 200, height: 200, min:0, max: 10000, majorTicks: ['0', '2', '4', '6', '8', '10']}
  }

  var motor_html_element = {
    volt: new google.visualization.Gauge(document.getElementById('motor_volt_div')),
    current: new google.visualization.Gauge(document.getElementById('motor_current_div')),
    power: new google.visualization.Gauge(document.getElementById('motor_power_div')),
    freq: new google.visualization.Gauge(document.getElementById('motor_freq_div')),
    rpm: new google.visualization.Gauge(document.getElementById('motor_rpm_div'))
  };

  motor_html_element.volt.draw(motor_chart.volt, motor_options.volt);
  motor_html_element.current.draw(motor_chart.current, motor_options.current);
  motor_html_element.power.draw(motor_chart.power, motor_options.power);
  motor_html_element.freq.draw(motor_chart.freq, motor_options.freq);
  motor_html_element.rpm.draw(motor_chart.rpm, motor_options.rpm);


  var misc_chart = {
    discharge: google.visualization.arrayToDataTable([['Label', 'Value'],['Discharge (LPM)', 0]]),
    energy: google.visualization.arrayToDataTable([['Label', 'Value'],['Energy (KWH)', 0]]),
    volt: google.visualization.arrayToDataTable([['Label', 'Value'],['Volt (V)', 0]]),
    current: google.visualization.arrayToDataTable([['Label', 'Value'],['Current (A)', 0]]),
    power: google.visualization.arrayToDataTable([['Label', 'Value'],['Power (KW)', 0]])
  }

  var misc_options = {
    discharge: {width: 200, height: 200, min:0, max: 1000, majorTicks: ['0', '200', '400', '600', '800', '1000']},
    energy: {width: 200, height: 200, min:0, max: 10, majorTicks: ['0', '2', '4', '6', '8', '10']},
    volt: {width: 200, height: 200, min:0, max: 1000, majorTicks: ['0', '200', '400', '600', '800', '1000']},
    current: {width: 200, height: 200, min:0, max: 20, majorTicks: ['0', '5', '10', '15', '20']},
    power: {width: 200, height: 200, min:0, max: 20, majorTicks: ['0', '5', '10', '15', '20']}
  }

  var misc_html_element = {
    discharge: new google.visualization.Gauge(document.getElementById('motor_today_discharge')),
    energy: new google.visualization.Gauge(document.getElementById('motor_current_div')),
    volt: new google.visualization.Gauge(document.getElementById('motor_power_div')),
    current: new google.visualization.Gauge(document.getElementById('motor_freq_div')),
    power: new google.visualization.Gauge(document.getElementById('motor_rpm_div'))
  };

  misc_html_element.discharge.draw(misc_chart.discharge, misc_options.discharge);

  socket.on('news', function (data) {
    document.getElementById('rmuno').innerText = data['rmuno'];
    document.getElementById('modemno').innerText = data['modemno'];
    document.getElementById('modemip').innerText = data['modemip'];
    document.getElementById('tele').innerText = data['tele'];
    document.getElementById('readdate').innerText = data['readdate'];
    document.getElementById('rtcdate').innerText = data['rtcdate'];
    motor_chart.volt.setValue(0, 1, '100');
    motor_chart.current.setValue(0, 1, '10');
    motor_chart.power.setValue(0, 1, '15');
    motor_chart.freq.setValue(0, 1, '200');
    motor_chart.rpm.setValue(0, 1, '6000');
    motor_html_element.volt.draw(motor_chart.volt, motor_options.volt);
    motor_html_element.current.draw(motor_chart.current, motor_options.current);
    motor_html_element.power.draw(motor_chart.power, motor_options.power);
    motor_html_element.freq.draw(motor_chart.freq, motor_options.freq);
    motor_html_element.rpm.draw(motor_chart.rpm, motor_options.rpm);
  });
  // setInterval(function() {
  //   data.setValue(0, 1, 40 + Math.round(60 * Math.random()));
  //   chart.draw(data, options);
  // }, 13000);
  // setInterval(function() {
  //   data.setValue(1, 1, 40 + Math.round(60 * Math.random()));
  //   chart.draw(data, options);
  // }, 5000);
  // setInterval(function() {
  //   data.setValue(2, 1, 500);
  //   chart.draw(data, options);
  // }, 26000);
}
</script>

